class Solution:
    def numberOfPairs(self, points: List[List[int]]) -> int:
        n_possibilities = 0
        points = sorted(points, key=lambda x: (x[1], -x[0]))
        print(points)
        n = len(points)
        for i in range(n):
            curr_point = points[i]
            min_x = -float('inf')
            for j in range(i+1, n):
                x, y = points[j]
                if x > min_x and x <= curr_point[0]:
                    min_x = max([x, min_x])
                    n_possibilities +=1
        return n_possibilities
