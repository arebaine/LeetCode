class Solution:
    def sortVowels(self, s: str) -> str:
        vowels = "aeiouAEIOU"
        word = list(s)
        vowels_pos = []
        present_vowels = []
        for i, el in enumerate(word):
            if el in vowels:
                vowels_pos.append(i)
                present_vowels.append(ord(el))
        present_vowels.sort()
        for i,car_code in enumerate(present_vowels):
            word[vowels_pos[i]] = chr(car_code)
        return "".join(word)
