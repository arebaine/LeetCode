class Solution:
    def maxProfit(self, prices: List[int]) -> int:
        max_profit = 0
        i = 1
        current_buy = prices[0]
        while i < len(prices):
            current_price = prices[i]
            if current_price < current_buy:
                current_buy = current_price
            else:
                if current_price > current_buy:
                    profit = current_price - current_buy
                    max_profit = max([profit, max_profit])
            i+=1
        return max_profit
